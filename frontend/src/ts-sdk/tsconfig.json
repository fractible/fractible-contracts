

{
  "compilerOptions": {
    "lib": ["es5", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "target": "es5",
    "downlevelIteration": true,
    "declaration": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "include": [ "test.ts", "src/*.ts", "src/*_code.json" ]
}
